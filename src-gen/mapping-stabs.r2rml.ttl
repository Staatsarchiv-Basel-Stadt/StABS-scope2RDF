		rr:objectMap [
			rr:constant <https://www.ica.org/standards/RiC/vocabularies/recordSetTypes#Fonds> ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate rico:isOrWasIncludedIn ;
		rr:objectMap [
			rr:template "http://ld.staatsarchiv.bs.ch/RecordResource/{PARENT_ID_NR}" ;
		];
	]
.
